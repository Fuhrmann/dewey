{
  "manifest_version": 2,
  "name": "Dewey for Chrome",
  "version": "1.1.2",

  "description": "Dewey makes finding and tagging your Chrome bookmarks quick and easy",

  "permissions": [
    "bookmarks", "storage"
  ],

  "app": {
    "launch": {
      "local_path": "app.html"
    }
  },

  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'"
}