{
  "manifest_version": 2,
  "name": "Bookmarks",
  "version": "1.1.2",

  "description": "Bookmarks makes finding and tagging your Chrome bookmarks quick and easy",

  "permissions": [
    "bookmarks", "storage"
  ],

  "app": {
    "launch": {
      "local_path": "app.html"
    }
  },

  "options_page": "options/index.html",

  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}